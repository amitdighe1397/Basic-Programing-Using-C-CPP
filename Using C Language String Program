}
printf(" count= %d", count);
}

4 Write a c program to accept string and print it in the revers order.
#include<stdio.h>
void main()
{
char str[100];
int i=0,count=0;
printf("Enter a string:");
fgets(str, sizeof(str), stdin);
printf("your string :%s ", str);
while (str[i]!='\0')
{
count++;
i++;
}
printf("first given string revers");
i = count;
while (i>=0)
{
printf("%c", str[i]);
i--;
}
}

5 Write a c program to count count of number of vowels and number of consonants in the given 
string
#include<stdio.h>
void main()
{
char str[100];
int i = 0,vow=0,con=0;
printf("Enter a string:");
fgets(str, sizeof(str), stdin);
printf("your string :%s ", str);
while (str[i] != '\0')
{
if (str[i] == ' ')
{
i++;
}
if (str[i] == 'a' || str[i]=='A' || str[i] == 'e' || str[i] == 'E' || str[i] == 'i' || str[i] == 'I'
|| str[i] == 'o' || str[i] == 'O' || str[i] == 'u' || str[i] == 'U')
{
vow++;
}
else
{
con++;
}
i++;
}
printf("vowels= %d\n", vow);
printf("consonunt= %d\n", con);
}

6 Write a c program to revers a given string as below
#include<stdio.h>
void main()
{
char str[100];
int i = 0,j=0;
printf("Enter a string:");
fgets(str, sizeof(str), stdin);
printf("your string :%s ", str);
while (str[i] != '\n')
{
while (str[i] == ' ' && str[i] != '\n')
{
i++;
}
while (str[i] != ' ' && str[i] != '\n')
{
if (str[i] == ' ')
{
printf(" ");
}
i++;
}
j = i - 1;
while (str[i] != ' ' && str[i] >= 0)
{
printf("%c", str[j]);
j--;
}
}
}

7 write a c program to replace space with '$' in given string
#include<stdio.h>
void main()
{
char str[100];
int i=0;
printf("Enter a string:");
fgets(str, sizeof(str), stdin);
printf("your string :%s ", str);
while (str[i] != '\0')
{
if (str[i] == ' ')
{
printf("$");
i++;
}
if (str[i] == ' ')
{
i++;
}
printf("%c", str[i]);
i++;
}
}

