16 Write a c program which accept sentance form user and position from user and print the word 
at that position
#include<stdio.h>
void main()
{
char str[100];
int i = 0, n, count = 1;
printf("Enter a string:");
fgets(str, sizeof(str), stdin);
printf("enter a position:");
scanf_s("%d", &n);
while (str[i] != '\0')
{
if (str[i] == ' ')
{
count++;
i++;
}
if (n == count)
{
printf("position= &d", n);
printf("words= %", count);
}
i++;
}
}

 17 Write a c program to convert the string from upper case to lower case.
#include<stdio.h>
void main()
{
char str[100];
int i = 0;
printf("Enter a string:");
fgets(str, sizeof(str), stdin);
printf("your string:%s", str);
while (str[i] != '\0')
{
if (str[i] >= 'A' && str[i] <= 'Z')
{
str[i] = str[i] + 32;
}
i++;
}
printf("lower case string= %s", str);
}

18 Write a c program which toggles the case of a string
#include<stdio.h>
void main()
{
char str[100];
int i = 0;
printf("Enter a string:");
fgets(str, sizeof(str), stdin);
printf("your string:%s", str);
while (str[i] != '\0')
{
if (str[i] >= 'a' && str[i] <= 'z')
{
str[i] = str[i] - 32;
i++;
}
if (str[i] >= 'A' && str[i] <= 'Z')
{
str[i] = str[i] + 32;
i++;
}
i++;
}
printf("Upper case string= %s", str);
}

19.Write a C program to check whether given strings are Anagram strings or not.
#include<stdio.h>
void main()
{
char str[100];
int i = 0, j = 0, count = 0, count1 = 0, index = 0;
printf("Please enter a String\n");
fgets(str, sizeof(str), stdin);
printf("\nGiven string is \n%s", str);
while(str[i]!='\n')
{
while (str[i] == ' ' && str[i] != '\n')
{
i++;
}
index = i; count = 0;
while (str[i] != ' ' && str[i] != '\n')
{
count++; i++;
}
if (count % 2 == 0)
{
j = i - 1;
while (str[j] != ' ' || j == 0)
{
printf("%c", str[j]);
j--;
}
}
else
{
j = index;
while (str[j] != ' ' && str[j] != '\n')
{
printf("%c", str[j]);
j++;
}
}
if (str[i] == ' ')
{
count1 = 0; j = i;
while (str[j] != '\n')
{
if (str[j] != ' ')
{
count1 = 1;
break;
}
j++;
}
}
if (count1 == 1)
{
printf(" ");
}
}
}

20 Write a c program which accept string from user and copy that string into some another string.
#include<stdio.h>
void main()
{
char str1[100];
char str2[100];
int i = 0;
printf("Enter a string:");
fgets(str1, sizeof(str1), stdin);
printf("your string:%s", str1);
while (str1[i] != '\0')
{
str2[i] = str1[i];
i++;
}
str2[i] = '\0';
printf(" copy string= %s", str2);
}

21 Write a c program which accept string from user and copy first n characters into some 
distination string
#include<stdio.h>
void main()
{
char str[100];
int i = 0,n;
printf("Enter a string:");
fgets(str, sizeof(str), stdin);
printf("your string:%s", str);
printf("Enter a value of n:");
scanf_s("%d", &n);
for(i=0;i<=n;i++)
{
printf("%c", str[i]);
}
}

