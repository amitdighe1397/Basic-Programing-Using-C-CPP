fgets(str1, sizeof(str1), stdin);
printf("your string:%s", str1);
printf("Enter a second string:");
fgets(str2, sizeof(str2), stdin);
while (str1[i] != '\n')
{
if (str1[i] == ' ')
{
i++;
}
printf("%c", str1[i]);
i++;
}
while (str2[j] != '\n')
{
if (str2[j] == ' ')
{
j++;
}
printf("%c", str2[j]);
j++;
}
}

24.Write a C program which accept two strings from user and append N characters of second 
string after first string.
#include<stdio.h>
void main()
{
char str1[100];
char str2[100];
int i = 0, j = 0, n;
printf("Enter a first string:");
fgets(str1, sizeof(str1), stdin);
printf("Enter a second string:");
fgets(str2, sizeof(str2), stdin);
printf("Enter a value of n:");
scanf_s("%d", &n);
while (str1[i] != '\n')
{
if (str1[i] == ' ')
{
i++;
}
printf("%c", str1[i]);
i++;
}
for (j = 0; j<n; j++)
{
printf("%c", str2[j]);
}
}

25.Write a C program which accept two strings from user and compare two strings. If both strings 
are equal then return 0 otherwise return difference between first mismatch character.
#include<stdio.h>
void main()
{
char str1[100];
char str2[100];
int i = 0, j = 0;
printf("Enter a first string:");
fgets(str1, sizeof(str1), stdin);
printf("your string:%s", str1);
printf("Enter a second string:");
fgets(str2, sizeof(str2), stdin);
printf("your string:%s", str2);
while (str1[i] != '\n')
{
if (str1[i] == str2[j])
{
printf("Both are equal");
break;
}
else
{
printf("both are Not equal");
break;
}
i++;
j++;
}
}

 27.Write a C program which accept two strings from user and compare two strings without case 
sensitivity. If both strings are equal then return 0 otherwise return difference between first 
mismatch character.
#include<stdio.h>
void main()
{
char str1[100];
char str2[100];
int i = 0, j = 0;
printf("Enter a first string:");
fgets(str1, sizeof(str1), stdin);
printf("your string:%s", str1);
printf("Enter a second string:");
fgets(str2, sizeof(str2), stdin);
printf("your string:%s", str2);
while (str1[i] != '\n')
{
if (str1[i]==str2[j])
{
i++;
j++;
printf("both are equal");
break;
}
else
{
printf("0");
break;
}
i++;
j++;
}
}

28.Write a C program which accept string from user and then reverse the string till first N 
characters without taking another string.
#include<stdio.h>
void main()
{
char str[100];
int i = 0;
int n;
int temp;
printf("enter a string\n");
fgets(str, sizeof(str), stdin);
printf("enter n no of characters revers\n");
scanf("%d", &n);
while (str[i] != '\0')
{
while (i <= n && n >= i)
{
temp = str[i];
str[i] = str[n];
str[n] = temp;
i++;
n--;
}
i++;
}
i = 0;
while (str[i] != '\0')
{
printf("%c", str[i]);
i++;
}
}

 29.Write a C program which accept string from user and then accept range and reverse the string 
in that range without taking another string.
#include<stdio.h>
void main()
{
char str[100];
int i = 0, n1, n2,temp;
printf("enter a string\n");
fgets(str, sizeof(str), stdin);
printf("enter n1\n");
scanf("%d", &n1);
printf("enter n2\n");
scanf("%d", &n2);
while (str[i] != '\0')
{
while (n1 <= n2 && n2 >= n1)
{
temp = str[n1];
str[n1] = str[n2];
str[n2] = temp;
n1++;
n2--;
}
i++;
}
i = 0;
while (str[i] != '\0')
{
printf("%c", str[i]);
i++;
}
}

 30.Write a C program which accept string from user and reverse words from that string which are 
of even length.*
#include<stdio.h>
void main()
{
char str[100];
char s[100];
int count = 0, i, j;
printf("Enter the string\n");
fgets(str, sizeof(str), stdin);
i = 0;
j = 0;
while (str[i] != '\0')
{
j = 0;
while (str[i] != ' ' && str[i] != '\n')
{
s[j] = str[i];
count++;
i++;
j++;
}
if (count % 2 == 0)
{
j = i - 1;
while (str[j] != ' ' && j >= 0)
{
printf("%c", str[j]);
j--;
}
}
else
{
s[j] = '\0';
j = 0;
while (s[j] != '\0')
{
printf("%c", s[j]);
j++;
}
}
printf(" ");
i++;
}
}

 31.Write a C program which accept string from user and checkwhether string is palindrome or 
not.
#include <stdio.h>
void main()
{
char str[100];
int i=0,j;
printf("Enter any string: ");
fgets(str,sizeof(str),stdin);
while (str[i] != '\0')
{
i++;
}
i = 0;
j= i - 1;
while (i <= j)
{
if (str[i] != str[j])
break;
i++;
j--;
}
if (i >= j)
{
printf("String is Palindrome.");
}
else
{
printf("String is Not Palindrome.");
}
}

32.Write a C program to count number of alphabates, spacesand words in given string.
#include<stdio.h>
void main()
{
char str[100];
int i = 0, space = 1, words = 1, alphabets = 0;
printf("Enter a string:");
fgets(str, sizeof(str), stdin);
printf("your string:%s", str);
while (str[i] != '\n')
{
if (str[i] == ' ')
{
space++;
}
else if (str[i]!=' ')
{
alphabets++;
}
if (str[i] == ' ')
{
words++;
}
i++;
}
printf("space= %d\n", space);
printf("alphabet= %d\n", alphabets);
printf("word= %d\n", word);
}
