	Printf(“the value of a is %d\n”,b);
	}

Q14:Write a C program to calculate sum of digits of a number.
#include<stdio.h>
Void main()
{
	Int I,n,sum=0;
	Printf(“enter the number”);
	Scanf(“%d”,&n);
	While(n>0)
	{
		i=n%10;
		Sum=sum+i;
		n=n/10;
	}
	Printf(“sum is:%d\n”,sum);
}

Q15: Write a C program to calculate product of digits of a number.
#include<stdio.h>
Void main()
{
	Int I,n,mul=1;
	Printf(“enter the number”);
	Scanf(“%d”,&n);
	While(n>0)
	{
		i=n%10;
		mul=mul*i;
		n=n/10;
	}
	Printf(“sum is:%d\n”,mul);
} 

Q16:Write a C program to enter a number and print its reverse.

#include<stdio.h>
Void main()
{
	Int n,i,reverse=0;
	Printf(“enter the number”);
	Scanf(“%d”,&n);
	While(n>0)
	{	i=n%10;
	reverse=reverse*10+i;
	n=n/10;
	}
	Printf(“reverse number is:%d\n”,reverse);
} 

Q17:Write a C program to check whether a number is palindrome or not
#include<stdio.h>
Void main()
{
	Int n,I,reverse=0,temp;
	Printf(“enter the number”);
	Scanf(“%d”,&n);
	Temp=n;
	While(n>0)
	{	i=n%10;
	Reverse=reverse*10+I;
	N=n/10;
	}
	Printf(“reverse number is:%d\n”,reverse);
	If(temp==reverse)
	Printf(“the number is palindrome”);
	Else
		Printf(“the number is not palindrome”);
} 

