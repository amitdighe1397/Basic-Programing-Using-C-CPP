23. Write a C program to calculate factorial of a number.
#include<stdio.h>
Void main()
{
	Int I,x,ans=1;
	Printf(“enter number”);
	Scanf(“%d”,&x);

	For (i=1;i<=x;i++)
	{
		Ans=ans*I;
		}
  		Printf(“%d”,ans);
  		}

24. Write a C program to find HCF (GCD) of two Numbers.
#include<stdio.h>
Void main()
{
Char I,n1,n2,ans,min;
Printf(“enter the n1 and n2”);
Scanf(“%d%d”,&n1,&n2);	
If(n1<n2)
{
	Min=n1;
	}
	Else
	Min=n1;
	For(i=1;i<=min;i++)
	If(n1%i==0&& n2%i==0)
	Ans=I;
Printf(“%d”,ans);
	} 


25. Write a C program to find LCM of two numbers.

#include<stdio.h>
Void main()
{
	Int I,n1,n2,max,lcm;
	Printf(“entet first number”);
	Scanf(“%d”,&n1);
	Printf(“enter second number”);
	Scanf(“%d”,&n2);
	
	If(n1<n2)
	{
		Max=n2;
	}
	Else
	{
 		Max=n1;
	}
	While(1)
	{
		If (max%n1==0  &&  max%n2==0)
		{
			Printf(“%d”,max);
			Break;
		}
Max++;
}
	}
Q26. Write a C program to check whether a number is Prime number or not.

#include<stdio.h>
void main()
{
	int i,n,count=0;
	printf("enter the number");
	scanf("%d",&n);
	for(i=1;i<=n;i++)
		if(n%i==0)
		count++;
	{	
	if(count==2)
		printf("number is prime");
		else 
		printf("number is not prime");    
	}
	} 
Q27. Write a C program to print all Prime numbers 
Between 1 to n.

#include<stdio.h>
void main()
{
	int i,n,count=0,j;
	printf("enter the number");
	scanf("%d",&n);
	for(i=1;i<=n;i++)
	{
			count=0;
			for(j=1;j<=i;j++)
			{	
		if(i%j==0)
		{
		count++;
		}
			}	
	if(count==2)
	{
		printf("%d\n",i);    
	}
			}
	} 
28. Write a C program to find sum of all prime numbers Between 1 to n.

#include<stdio.h>
void main()
{
	int i,n,count=0,j,sum=0;
	printf("enter the number");
	scanf("%d",&n);
	for(i=1;i<=n;i++)
	{
		count=0;
			for(j=1;j<=i;j++)
			{	
		if(i%j==0)
		{
		count++;
		}
			}
	if(count==2)
	{
		sum=sum+i;
	}	
		}
		printf("%d\n",sum);
	} 
  
